<div class="teacher-container">
  <h2><%= current_teacher.prefix %> <%= current_teacher.name %>'s Assignment Dashboard</h2>

  <div class="dashboard">
    <h3><%= @type.name %></h3>

      <% @assignments.each do |assignment| %>
    <div class="blocks">
        <% assignment.students.each do |pair| %>
        <li><%= image_tag('pear-white.png') %><%= pair.name %></li>
        <% end %>
    </div>
      <% end %>
  </div>
  </div>
